        <script>
        function processCriteo377600 (json) {
          console.log('API response: ',json);
          if (json.response_status != 0) {
            /* call passback function here*/
            return;
          }
          var div = document.getElementById('criteo-377600');
          var htmlcode = '';
          var products_count = 3;
          htmlcode += '<div class="criteo-title kop-judul"><h2>BELI SEKARANG</h2></div>';
          htmlcode += '<div class="criteo-privacy">';
          htmlcode += '<a href="'+ json.privacy.optout_click_url +'" target="_blank"><img src="'+ json.privacy.optout_image_url + '"></a>';
          htmlcode += '</div>';
          htmlcode += '<div class="criteo-products content-current">';
          for (var i in json.products) {
            if (i >= products_count) break;
            htmlcode += '<div class="criteo-product wp-conten-terkini">';
            htmlcode += '<a href="' + json.products[i].click_url + '" target="_blank">';
            htmlcode += '<div class="criteo-product-image img-ct">';
            htmlcode += '<img src="'+ json.products[i].image.url +'">';
            htmlcode += '</div>';
            htmlcode += '<div class="text">';
            htmlcode += '<div class="criteo-product-title jdl-ct-a"><h3>'+json.products[i].title+'</h3></div>';
            htmlcode += '<div class="criteo-advertiser jdl-ct-a">';
            htmlcode += '<a href="' + json.advertiser.logo_click_url + '" target="_blank">'+ json.advertiser.domain +'</a>';
            htmlcode += '</div>';
            htmlcode += '</div>';
            htmlcode += '</a>';
            htmlcode += '<div style="clear:both"></div>';
            htmlcode += '</div>';
          }
          htmlcode += '</div>';
          for (i in json.impression_pixels) {
            htmlcode += '<img src="' + json.impression_pixels[i].url + '" width="0px" height="0px" style="display:none" >';
          }
          div.innerHTML = htmlcode;
          div.style.display = 'block';
        };
        </script>
